<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_sale_order_line_inherit_serial_number" model="ir.ui.view">
        <field name="name">sale.order.line.inherit</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='order_line']/tree/field[@name='product_template_id']" position="after">
                <field name="launch_date" column_invisible="True" nolabel="1"/>
                <!-- <field name="product_code" /> -->
                <field name="image" widget="image" options="{'size': [60, 60]}"/>

                <!-- <field name="image" /> -->
                <field name="hs_code_id" column_invisible="True" nolabel="1"/>
                <field name="packaging_detail_id" column_invisible="True" nolabel="1"/>
                <field name="length" column_invisible="True" nolabel="1"/>
                <field name="width" column_invisible="True" nolabel="1"/>
                <field name="height" column_invisible="True" nolabel="1"/>
                <field name="net_weight" column_invisible="True" nolabel="1"/>
                <field name="gross_weight" column_invisible="True" nolabel="1" />
                <field name="cbm" column_invisible="True" nolabel="1"/>
                <field name="order_cbm" column_invisible="True" nolabel="1"/>
                <field name="fcl_20" column_invisible="True" nolabel="1"/>
                <field name="fcl_40" column_invisible="True" nolabel="1"/>
                <field name="shelf_life" column_invisible="True" nolabel="1"/>
                <field name="custom_price" column_invisible="True" nolabel="1" />
                <field name="no_of_ctn" column_invisible="True" nolabel="1" />
                <field name="lbs_oz" column_invisible="True" nolabel="1" />

            </xpath>
        </field>
    </record>
</odoo>
